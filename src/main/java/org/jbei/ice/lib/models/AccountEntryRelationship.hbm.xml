<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
    <class name="AccountEntryRelationship" table="account_entry_relationship">
        <id name="id" column="id" type="int">
            <generator class="sequence">
                <param name="sequence">account_entry_relationship_id_seq</param>
            </generator>
        </id>
        <many-to-one name="account" column="accounts_id" class="Account"></many-to-one>
        <many-to-one name="entry" column="entries_id" class="Entry"></many-to-one>        
    </class>
</hibernate-mapping>
