<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
    <class name="FeatureRelationship" table="feature_relationship">
        <id name="id" column="id" type="int">
            <generator class="sequence">
                <param name="sequence">feature_relationship_id_seq</param>
            </generator>
        </id>
        <property name="name" column="name" type="string" length="128" />
        <property name="description" column="description" type="string" length="1023"/>
        <property name="ontology" column="ontology" type="string" length="128" />
    </class>
</hibernate-mapping>
