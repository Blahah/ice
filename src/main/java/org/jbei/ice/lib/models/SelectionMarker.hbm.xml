<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="SelectionMarker" table="selection_markers">
		<id name="id" column="id" type="int">
            <generator class="sequence">
                <param name="sequence">selection_markers_id_seq</param>
            </generator>
		</id>
		<property name="name" column="name" type="string" not-null="true" length="50" />
		<many-to-one name="entry" column="entries_id" class="Entry" not-null="true" />
	</class>
</hibernate-mapping>
