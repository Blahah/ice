<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="AccountPreferences" table="account_preferences">
		<id name="id" column="id" type="int">
	       <generator class="sequence">
                <param name="sequence">accounts_preferences_id_seq</param>
            </generator>
		</id>
		<property name="preferences" column="preferences" type="text" />
		<property name="restrictionEnzymes" column="restriction_enzymes"
			type="text" />
		<many-to-one name="account" column="accounts_id" class="Account"
			not-null="true" unique="true" />
	</class>
</hibernate-mapping>
