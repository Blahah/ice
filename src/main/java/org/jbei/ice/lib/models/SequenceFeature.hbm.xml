<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
    <class name="SequenceFeature" table="sequence_feature">
        <id name="id" column="id" type="int">
            <generator class="sequence">
                <param name="sequence">sequence_feature_id_seq</param>
            </generator>
        </id>
        <property name="start" column="feature_start" type="int" />
        <property name="end" column="feature_end" type="int" />
        <property name="strand" column="strand" type="int" />
        <property name="name" column="name" type="string" length="127" />
        <many-to-one name="sequence" column="sequence_id" class="Sequence"></many-to-one>
        <many-to-one name="feature" column="feature_id" class="Feature"></many-to-one>        
    </class>
</hibernate-mapping>
