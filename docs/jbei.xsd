<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	targetNamespace="http://jbei.org/ice"
	xmlns:ice="http://jbei.org/ice"
	xmlns:seq="http://jbei.org/sequence">
<xs:import namespace="http://jbei.org/sequence" schemaLocation="seq.xsd"/>
<xs:element name="ice">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="ice:record_id"/>
			<xs:element ref="seq:sequence"/>	
		</xs:sequence>
	</xs:complexType>
</xs:element>
<xs:element name="record_id" type="xs:string"/>
</xs:schema>