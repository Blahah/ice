<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="Feature" table="features">
		<id name="id" column="id" type="int">
			<generator class="sequence">
				<param name="sequence">features_id_seq</param>
			</generator>
		</id>

		<property name="name" column="name" type="string" length="127" />
		<property name="description" column="description" type="string"
			length="1023" />
		<property name="identification" column="identification" type="string"
			length="127" />
		<property name="uuid" column="uuid" type="string" length="36" />
		<property name="autoFind" column="auto_find" type="int" />
		<property name="genbankType" column="genbank_type" type="string"
			length="127" />

	</class>
</hibernate-mapping>
