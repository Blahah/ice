<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="Link" table="links">
		<id name="id" column="id" type="int">
			<generator class="sequence">
				<param name="sequence">links_id_seq</param>
			</generator>
		</id>
		<property name="link" column="link" type="string" length="40" />
		<property name="url" column="url" type="string" length="255" />
		<many-to-one name="entry" column="entries_id" class="Entry" not-null="true" />
	</class>
</hibernate-mapping>
