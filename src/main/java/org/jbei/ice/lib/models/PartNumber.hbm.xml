<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="PartNumber" table="part_numbers">
		<id name="id" column="id" type="int">
			<generator class="native">
			</generator>
		</id>
		<property name="partNumber" column="part_number" type="string"
			not-null="true" length="127" />
		<many-to-one name="entry" column="entries_id" class="Entry"
			not-null="true" />
	</class>
</hibernate-mapping>
