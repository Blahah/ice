<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="Attachment" table="attachments">
		<id name="id" column="id" type="int">
			<generator class="sequence">
				<param name="sequence">attachments_id_seq</param>
			</generator>
		</id>
		<property name="description" column="description" type="text"
			not-null="true" />
		<property name="fileName" column="filename" type="string"
			length="255" not-null="true" />
		<property name="fileId" column="file_id" type="string"
			length="36" not-null="true" />
		<many-to-one name="entry" column="entries_id" class="Entry"
			not-null="true" />
	</class>
</hibernate-mapping>
