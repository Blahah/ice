<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
    <class name="Sequence" table="sequences">
        <id name="id" column="id" type="int">
            <generator class="native">
            </generator>
        </id>
        <property name="sequence" column="sequence" type="text" />
        <property name="sequenceUser" column="sequence_user" type="text" />
        <property name="fwdHash" column="fwd_hash" type="string" length="40" />
        <property name="revHash" column="rev_hash" type="string" length="40"/>
        <many-to-one name="entry" column="entries_id" class="Entry"
            not-null="true" unique="true"/>
        <set name="sequenceFeatures" lazy="true" order-by="id" inverse="true">
            <key column="sequence_id" />
            <one-to-many class="SequenceFeature" />
        </set>
    </class>
</hibernate-mapping>
