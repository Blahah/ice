<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="Vote" table="votes">
		<id name="id" column="id" type="int">
			<generator class="sequence">
				<param name="sequence">votes_id_seq</param>
			</generator>
		</id>
		<many-to-one name="account" column="accounts_id" class="Account"
			not-null="true" />
		<many-to-one name="entry" column="entries_id" class="Entry"
			not-null="true" />
		<property name="score" column="score" type="int" not-null="true" />
		<property name="comment" column="comment" type="text"
			not-null="true" />
		<property name="creationTime" column="creation_time" type="timestamp" />
	</class>
</hibernate-mapping>
