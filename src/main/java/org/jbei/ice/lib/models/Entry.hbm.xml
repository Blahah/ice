<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
	<class name="Entry" table="entries">
		<id name="id" column="id" type="int">
			<generator class="native">
				
			</generator>
		</id>
		<property name="recordId" column="record_id" type="string"
			length="36" not-null="true" />
		<property name="versionId" column="version_id" type="string"
			length="36" not-null="true" />
		<property name="recordType" column="record_type" type="string"
			length="10" not-null="true" />
		<property name="alias" column="alias" type="string" length="127" />
		<property name="owner" column="owner" type="string" length="127" />
		<property name="ownerEmail" column="owner_email" type="string"
			length="127" />
		<property name="creator" column="creator" type="string"
			length="127" />
		<property name="creatorEmail" column="creator_email" type="string"
			length="127" />
		<property name="visibility" column="visibility" type="int" />
		<property name="status" column="status" type="string" length="127" />
		<property name="keywords" column="keywords" type="string"
			length="127" />
		<property name="shortDescription" column="short_description"
			type="text" />
		<property name="longDescription" column="long_description"
			type="text" />
		<property name="references" column="literature_references" type="text" />
		<property name="creationTime" column="creation_time" type="timestamp" />
		<property name="modificationTime" column="modification_time"
			type="timestamp" />
		<set name="names" lazy="false" order-by="id" cascade="all" inverse="true">
			<key column="entries_id" />
			<one-to-many class="Name" />
		</set>
		<set name="partNumbers" lazy="false" order-by="id" cascade="all">
			<key column="entries_id" />
			<one-to-many class="PartNumber" />
		</set>
		<set name="links" lazy="false" order-by="id" cascade="all">
			<key column="entries_id" />
			<one-to-many class="Link" />
		</set>
		<set name="selectionMarkers" lazy="false" order-by="id" cascade="all">
			<key column="entries_id" />
			<one-to-many class="SelectionMarker" />
		</set>
        <one-to-one name="sequence" class="Sequence" property-ref="entry" />
	</class>
</hibernate-mapping>
