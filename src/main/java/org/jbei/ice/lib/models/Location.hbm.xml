<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
    <class name="Location" table="locations">
        <id name="id" column="id" type="int">
            <generator class="native">
            </generator>
        </id>
        <many-to-one name="sample" column="samples" class="Sample"
            not-null="true" />
        <property name="location" column="location" type="string" length="255" />
        <property name="barcode" column="barcode" type="string" length="255" />
        <property name="notes" column="notes" type="text" />
        <property name="wells" column="wells" type="string" length="255" />
        <property name="nColumns" column="n_columns" type="int" not-null="true"/>
        <property name="nRows" column="n_rows" type="int" not-null="true"/>
        <property name="creationTime" column="creation_time" type="timestamp" />
        <property name="modificationTime" column="modification_time" type="timestamp" />
    </class>
</hibernate-mapping>
