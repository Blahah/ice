<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbei.ice.lib.models">
    <class name="FeatureDNA" table="feature_dna">
        <id name="id" column="id" type="int">
            <generator class="sequence">
                <param name="sequence">feature_dna_id_seq</param>
            </generator>
        </id>
        <property name="hash" column="hash" type="string" length="40" not-null="true" />
        <property name="sequence" column="sequence" type="string" not-null="true" />
        <many-to-one name="feature" column="feature_id" class="Feature" unique="true" />
    </class>
</hibernate-mapping>
